#include <stdio.h>

int main()
{
    int a[10][10],i,j,p,q;
    printf("Enter no of rows in array(Max size-10):");
    scanf("%d",&p);
    printf("Enter no of columns in array(Max size-10):");
    scanf("%d",&q);
    printf("\nEnter the elements of the array:\n");
    for(i=0;i<p;i++)
    {
        for(j=0;j<q;j++)
            scanf("%d",&a[i][j]);
    }
    printf("\nEntered elements are:\n");
    for(i=0;i<p;i++)
    {
        for(j=0;j<q;j++)
        {
            printf("%d\t",a[i][j]);
        }   
        printf("\n");
    }
    return 0;
}
